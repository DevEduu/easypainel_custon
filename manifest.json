{
    "manifest_version": 3,
    "name": "EasyPainelCuston",
    "version": "1.2.1",
    "description": "Extensão para modificar elementos na página painel.receba.digital/monitor.",
    "permissions": ["scripting"],
    "host_permissions": ["https://painel.receba.digital/monitor"],
    "content_scripts": [
      {
        "matches": ["https://painel.receba.digital/monitor"],
        "js": ["content.js"]
      }
    ]
  }
  